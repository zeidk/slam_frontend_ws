{
  "name": "LOAM Scan Matching (Features + Odometry)",
  "version": 2,
  "layout": {
    "direction": "horizontal",
    "first": {
      "direction": "vertical",
      "first": {
        "id": "threeD",
        "type": "3D",
        "title": "3D View",
        "config": {
          "followMode": "follow-pose",
          "scene": {
            "transforms": { "frame": "odom" },
            "layers": [
              { "topic": "/tf", "visible": true },
              { "topic": "/tf_static", "visible": true },
              { "topic": "features/edge_cloud", "visible": true, "pointSize": 2 },
              { "topic": "features/planar_cloud", "visible": true, "pointSize": 2 },
              { "topic": "/trajectory", "visible": true }
            ]
          }
        }
      },
      "second": {
        "id": "raw",
        "type": "RawMessages",
        "title": "Messages",
        "config": {
          "topics": [
            "features/edge_cloud",
            "features/planar_cloud",
            "/odom",
            "/trajectory",
            "/tf",
            "/tf_static"
          ]
        }
      }
    },
    "second": {
      "direction": "vertical",
      "first": {
        "id": "odomPlot",
        "type": "Plot",
        "title": "Odometry XY",
        "config": {
          "paths": [
            { "value": "/odom.pose.pose.position.x", "label": "x" },
            { "value": "/odom.pose.pose.position.y", "label": "y" }
          ],
          "legend": true
        }
      },
      "second": {
        "id": "tfTree",
        "type": "TransformTree",
        "title": "TF Tree",
        "config": {}
      }
    }
  },
  "globalVariables": {}
}
